
<!-- views/contacts/index.ejs -->

<!DOCTYPE html>
<html>
  <head>
    <%- include('../partials/head') %> <!-- 1 -->
  </head>
  <body>
    <%- include('../partials/nav') %> <!-- 2 -->
    <div class="contact contact-index">
      <h2>Index</h2>
      <ul>
        <% var len = rows.length; %>
        <% for(var i =0; i < len; i++){ %>
        <li>
          <a href="/contacts/<%= rows[i].name%>"><%= rows[i].name %></a>
        </li>
        <% }; %>
      </ul>
    </div>
  </body>
</html>
